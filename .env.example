# Redis Configuration
# These settings will be overridden by values from Vault
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# HashiCorp Vault Configuration
VAULT_URL=http://localhost:8200
VAULT_TOKEN=your-token-here
VAULT_REDIS_PATH=secret/data/redis  # Path to Redis credentials in Vault

# Browser Configuration
HEADLESS=true

# Target Website
TARGET_URL=https://example.com
SITE_KEY=example

# Login Credentials (if needed)
SITE_USERNAME=your_username
SITE_PASSWORD=your_password
